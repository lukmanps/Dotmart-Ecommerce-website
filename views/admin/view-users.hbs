 <section>
 <section class="content-main">
            <div class="content-header">
                <div>
                    <h2 class="content-title card-title">Users List.</h2>
                    <p>User Management.</p>
                </div>
                <div>
                    <input type="text" placeholder="Search order ID" class="form-control bg-white">
                </div>
            </div>
            <div class="card mb-4">
                
                <div class="card-body">
                    <div class="table-responsive">
                        <table class="table table-hover">
                            <thead>
                                <tr>
                                    <th>#ID</th>
                                    <th scope="col">Name</th>
                                    <th scope="col">Email</th>
                                    <th scope="col">Mobile No.</th>
                                    <th scope="col">Wallet</th>
                                    <th scope="col">Status</th>
                                    <th scope="col" class="text-end"> Action </th>
                                </tr>
                            </thead>
                            <tbody>
                                {{#each users}}
                                <tr>
                                    <td>{{this._id}}</td>
                                    <td><b>{{this.registerName}}</b></td>
                                    <td>{{this.registerEmail}}</td>
                                    {{!-- <td><template class="__cf_email__" data-cfemail="aec3cfdcd8c7c0eecbd6cfc3dec2cb80cdc1c3">[email&#160;protected]</template></td> --}}
                                    <td>{{this.registerMobileno}}</td>
                                    <td>&#x20B9;{{this.wallet}}</td>
                                    {{#if status}}
                                    <td><span class="badge rounded-pill alert-success">Active</span></td>
                                    <td class="text-end">
                                        <a href="/admin/block-user/{{this._id}}" class="btn btn-md rounded font-sm">Block</a>
                                    
                                    </td>
                                    {{else}}
                                    <td><span class="badge rounded-pill alert-danger">Not Active</span></td>
                                    <td class="text-end">
                                        <a href="/admin/unblock-user/{{this._id}}" class="btn btn-md rounded font-sm">Unblock</a>
                                        
                                    </td>
                                    {{/if}}
                                </tr>
        
                                {{/each}}
                            </tbody>
                        </table>
                    </div> <!-- table-responsive //end -->
                </div> <!-- card-body end// -->
            </div> <!-- card end// -->
        </section> <!-- content-main end// -->
        <footer class="main-footer font-xs">
            <div class="row pb-30 pt-15">
                <div class="col-sm-6">
                    <script data-cfasync="false" src="../../../cdn-cgi/scripts/5c5dd728/cloudflare-static/email-decode.min.js"></script><script>
                    document.write(new Date().getFullYear())
                    </script>
                </div>
                <div class="col-sm-6">
                    <div class="text-sm-end">
                        All rights reserved
                    </div>
                </div>
            </div>
        </footer>
</section>
